{
  "ci": {
    "collect": {
      "staticDistDir": "./dist",
      "numberOfRuns": 3,
      "settings": {
        "preset": "desktop",
        "formFactor": "desktop"
      },
      "url": ["http://localhost/"]
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "aggregation": "median-run"
    }
  }
}
