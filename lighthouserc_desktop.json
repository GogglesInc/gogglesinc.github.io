{
  "ci": {
    "collect": {
      "staticDistDir": "./dist",
      "numberOfRuns": 3,
      "settings": {
        "preset": "desktop",
        "formFactor": "desktop",
        "outputPath": "./reports/lighthouse.desktop"
      },
      "url": ["http://localhost/"]
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./reports"
    }
  }
}
